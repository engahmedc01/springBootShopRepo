	<queryString>
		<![CDATA[SELECT
     SUM(PACKAGE_NUMBER) AS NET_WEIGHT,
     SUM(AMOUNT) AS TOTAL_COST,
       SELLER_WEIGHT.UNITE_PRICE AS unite_price
  
FROM
      SELLER_WEIGHT 
WHERE
     customer_order_id = $P{order_id}
GROUP BY
     unite_price 
order by unite_price DESC]]>
